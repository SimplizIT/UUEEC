<%= link_to 'Home page', root_path %><br><br>
<%= link_to 'Obligations page', obligations_path %><br><br>


 <%= form_for(@obligation) do |f| %>
  <div>
  <%= f.label :title %><br />
  <%= f.text_field :title %>
  </div>
  <div>
  <%= f.label :start %><br />
  <%= f.text_field :start, class: 'datepicker', :"data-autoclose" => true %>
  </div>
  <div>
  <%= f.label :end %><br />
  <%= f.text_field :end, class: 'datepicker' %>
  </div>
  <div>
  <%= f.label :allDay %><br />
  <%= f.check_box :allDay %>
  </div>
  <div>
  <%= f.label :description %><br />
  <%= f.text_area :description %>
  </div>
  <div>
  <%= f.submit %>
  </div>
  <% end %>