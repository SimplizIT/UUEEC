                  </article>
                  <!-- <div class="overlay">&nbsp;</div> -->
                </div>
              </div>
            </div>
          </section>
           <section class="sourse-gallery" style="margin-left: 418px;">
        </div>
      </div>



      <!-- This is where the left column starts -->
